      },
      "targets": [
        {
          "expr": "(sum(rate(http_server_requests_seconds_count{job=\"qawave-backend\", status=~\"5..\"}[5m])) / sum(rate(http_server_requests_seconds_count{job=\"qawave-backend\"}[5m]))) * 100",
